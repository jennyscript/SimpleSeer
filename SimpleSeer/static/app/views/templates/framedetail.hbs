
<div class="row framedetailrow">
        <div id="frame" class="span8">
                <div id="display"><img src="{{imgfile}}"><canvas id="displaycanvas"></canvas></div>
                <p class="frame_icons">
                        <a href="{{imgfile}}?download=True"><i class="icon-download"></i> Download</a>
                </p>
        </div>
        <div id="properties" class="span3">
                <table border="1" cellpadding="7">
                        <thead>

                        </thead>
                        <tbody>
                                <tr>
                                        <td>Camera</td>
                                        <td><b>{{camera}}</b></td>
                                </tr>
                                <tr>
                                        <td>Capture Time</td>
                                        <td>{{epochtime capturetime}}</td>
                                </tr>
                                <tr>
                                        <td>Capture Date</td>
                                        <td>{{epochdate capturetime}}</td>
                                </tr>
                                <tr>
                                        <td>Width</td>
                                        <td>{{width}} <span>pixels</span></td>
                                </tr>
                                <tr>
                                        <td>Height</td>
                                        <td>{{height}} <span>pixels</span></td>
                                </tr>
                        </tbody>
                </table>
        </div>
</div>


<div class="row" style="margin-bottom: 30px;">
  <div id="features" class="span12">
    {{#each featuretypes}}
        <h3>{{this.length}} {{this.[0].plural}} Detected</h3>
      <div class="set">{{featuredetail this}}</div>
    {{/each}}
  </div>
</div>


